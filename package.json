{
  "name": "@sunreal/f-icon",
  "version": "1.0.0",
  "description": "Feather for Vue.js",
  "main": "dist/main.umd.js",
  "module": "dist/index.es.js",
  "scripts": {
    "generate:test": "cross-env NODE_ENV=test node ./generate/generate.js",
    "generate": "cross-env NODE_ENV=prod node ./generate/generate.js",
    "build": "rimraf dist/* && rollup -c",
    "build:doc": "cross-env NODE_ENV=production webpack --config docs-src/webpack.config.js",
    "serve:test": "webpack-dev-server --open --config test/webpack.config.js",
    "serve": "cross-env NODE_ENV=development webpack-dev-server --open --config docs-src/webpack.config.js"
  },
  "keywords": ["feather", "vue"],
  "author": "fengyuan.sfy@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "change-case": "^4.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "feather-icons": "^4.28.0",
    "html-webpack-plugin": "^5.3.2",
    "less": "^4.1.1",
    "less-loader": "^10.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.3",
    "rollup-plugin-terser": "^7.0.2",
    "vue": "^2.6.14",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:sfengyuan/f-icon.git"
  },
  "bugs": {
    "url": "https://github.com/sfengyuan/f-icon/issues"
  }
}
